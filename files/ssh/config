Host *
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  AddKeysToAgent yes

  IgnoreUnknown UseKeychain
  UseKeychain yes

  ServerAliveInterval 30
  ServerAliveCountMax 120
  TCPKeepAlive yes

  ForwardAgent yes

  SendEnv LANG LC_*

  ControlPath /tmp/master-%r@%h:%p
  ControlMaster auto
  ControlPersist 1h
