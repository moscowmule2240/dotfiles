[user]
	name = moscowmule2240
	email = moscowmule2240@users.noreply.github.com
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
	quotepath = false
	precomposeunicode = true
	autocrlf = input
	symlinks = true
[push]
	default = current
[pack]
	threads = 2
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[merge]
	renameLimit = 999999
[diff]
	indentHeuristic = true
