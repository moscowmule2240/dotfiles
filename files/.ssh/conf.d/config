Host *
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  UseKeychain yes
  AddKeysToAgent yes

  ServerAliveInterval 30
  ServerAliveCountMax 120
  TCPKeepAlive yes

  IdentityFile ~/Workspace/dotfiles/dotmine/ssh/default/id_rsa
  ForwardAgent yes

  SendEnv LANG LC_*

Host *
  IdentityFile ~/Workspace/dotfiles/dotmine/ssh/default-no-passphrase/id_rsa

