Host *
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  UseKeychain yes

  ServerAliveInterval 30
  ServerAliveCountMax 120
  TCPKeepAlive yes

  IdentityFile ~/Workspace/dotfiles/dotmine/ssh/default/id_rsa
  ForwardAgent yes

Host *
  IdentityFile ~/Workspace/dotfiles/dotmine/ssh/gumi/id_rsa

